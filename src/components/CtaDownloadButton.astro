---
import { BookOpen } from "../assets/icons";

const {
  AriaLabel,
  CtaBtn,
  Url,
  CtaBtnTitle,
  IconColor,
} = Astro.props;
---

<a
  aria-label={AriaLabel}
  class="btn btn-secondary"
  href={Url}
  rel="noopener noreferrer"
  title={CtaBtnTitle}
  >
  <BookOpen iconFill={IconColor} /> {CtaBtn}
  </a
>
